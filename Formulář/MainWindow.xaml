<Window x:Class="Hesla.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Formulář"
        mc:Ignorable="d"
        Title="Form App" Height="450" Width="800">
    <Grid Background="Orange">
        <Grid.RowDefinitions>
            <RowDefinition Height="100*"/>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="160*"/>
            <RowDefinition Height="55*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="300*"/>
            <ColumnDefinition Width="10*"/>
            <ColumnDefinition Width="155*"/>
            <ColumnDefinition Width="10*"/>
            <ColumnDefinition Width="300*"/>
        </Grid.ColumnDefinitions>
        <Label Name="labelFn" Content="First Name:" FontSize="18" FontFamily="Arial Black" VerticalAlignment="Bottom" HorizontalAlignment="Right" Grid.Row="0" Grid.Column="0"></Label>
        <Label Name="labelLn" Content="Last Name:" FontSize="18" FontFamily="Arial Black" VerticalAlignment="Top" HorizontalAlignment="Right" Grid.Row="1" Grid.Column="0"></Label>
        <Label Name="labelBd" Content="Birth Date:" FontSize="18" FontFamily="Arial Black" VerticalAlignment="Top" HorizontalAlignment="Right" Grid.Row="2" Grid.Column="0"></Label>
        <Label Name="labelEd" Content="Highest Education:" FontSize="18" FontFamily="Arial Black" VerticalAlignment="Top" HorizontalAlignment="Right" Grid.Row="3" Grid.Column="0"></Label>
        <Label Name="labelJob" Content="Job:" FontSize="18" FontFamily="Arial Black" VerticalAlignment="Top" HorizontalAlignment="Right" Grid.Row="4" Grid.Column="0"></Label>
        <Label Name="labelPm" Content="Payment:" FontSize="18" FontFamily="Arial Black" VerticalAlignment="Top" HorizontalAlignment="Right" Grid.Row="5" Grid.Column="0"></Label>
        <Label Name="labelErrorFn" Foreground="Red" FontSize="12" FontFamily="Arial Black" VerticalAlignment="Bottom" HorizontalAlignment="Left" Grid.Row="0" Grid.Column="4" Margin="0,0,0,10"></Label>
        <Label Name="labelErrorLn" Foreground="Red" FontSize="12" FontFamily="Arial Black" VerticalAlignment="Bottom" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="4" Margin="0,6,0,0"></Label>
        <Label Name="labelErrorBd" Foreground="Red" FontSize="12" FontFamily="Arial Black" VerticalAlignment="Bottom" HorizontalAlignment="Left" Grid.Row="2" Grid.Column="4" Margin="0,6,0,0"></Label>
        <Label Name="labelErrorEd" Foreground="Red" FontSize="12" FontFamily="Arial Black" VerticalAlignment="Bottom" HorizontalAlignment="Left" Grid.Row="3" Grid.Column="4" Margin="0,6,0,0"></Label>
        <Label Name="labelErrorJob" Foreground="Red" FontSize="12" FontFamily="Arial Black" VerticalAlignment="Bottom" HorizontalAlignment="Left" Grid.Row="4" Grid.Column="4" Margin="0,6,0,0"></Label>
        <Label Name="labelErrorPm" Foreground="Red" FontSize="12" FontFamily="Arial Black" VerticalAlignment="Top" HorizontalAlignment="Left" Grid.Row="5" Grid.Column="4" Margin="0,10,0,0"></Label>
        <Label Name="labelLogin" FontSize="18" FontFamily="Arial Black" VerticalAlignment="Bottom" HorizontalAlignment="Left" Grid.Row="5" Grid.Column="0"></Label>
        <ComboBox Name="comboboxEd" Background="#FFFF5100" FontSize="14" FontFamily="Arial Black" VerticalAlignment="Bottom" HorizontalAlignment="Left" Grid.Row="3" Grid.Column="2" Margin="6">
            <ComboBoxItem>ZŠ</ComboBoxItem>
            <ComboBoxItem>SŠ</ComboBoxItem>
            <ComboBoxItem>VŠ</ComboBoxItem>
        </ComboBox>
        <TextBox Name="textboxFn" Background="#FFFF5100" FontSize="14" FontFamily="Arial Black" VerticalAlignment="Bottom" HorizontalAlignment="Center" Grid.Row="0" Grid.Column="2" Margin="0,0,0,10" MinWidth="150" MaxWidth="150" MinHeight="20" MaxHeight="20"></TextBox>
        <TextBox Name="textboxLn" Background="#FFFF5100" FontSize="14" FontFamily="Arial Black" VerticalAlignment="Top" HorizontalAlignment="Center" Grid.Row="1" Grid.Column="2" MinWidth="150" MaxHeight="150" Grid.RowSpan="2" Margin="0,6,0,0" MinHeight="20" MaxWidth="20"></TextBox>
        <TextBox Name="textboxBd" Background="#FFFF5100" FontSize="14" FontFamily="Arial Black" VerticalAlignment="Top" HorizontalAlignment="Center" Grid.Row="2" Grid.Column="2" MinWidth="150" MaxHeight="150" Grid.RowSpan="2" Margin="0,6,0,0" MinHeight="20" MaxWidth="20"></TextBox>
        <TextBox Name="textboxJob" Background="#FFFF5100" FontSize="14" FontFamily="Arial Black" VerticalAlignment="Top" HorizontalAlignment="Center" Grid.Row="4" Grid.Column="2" MinWidth="150" MaxHeight="150" Grid.RowSpan="2" Margin="0,6,0,0" MinHeight="20" MaxWidth="20"></TextBox>
        <TextBox Name="textboxPm" Background="#FFFF5100" FontSize="14" FontFamily="Arial Black" VerticalAlignment="Top" HorizontalAlignment="Center" Grid.Row="5" Grid.Column="2" MinWidth="150" MaxHeight="150" Grid.RowSpan="2" Margin="0,6,0,0" MinHeight="20" MaxWidth="20"></TextBox>
        <Button Name="button1" Content="Login " FontSize="18" FontFamily="Arial Black" VerticalAlignment="Bottom" HorizontalAlignment="Center" Grid.Row="5" Grid.Column="2" MinWidth="100" Margin="0,0,0,5" Padding="11,1,1,1" BorderThickness="5,3,5,3" ClipToBounds="True" Background="#FFFF5100" Click="Button_Click"></Button>
        <Button Name="button2" Content="End Program" FontSize="18" FontFamily="Arial Black" VerticalAlignment="Bottom" HorizontalAlignment="Center" Grid.Row="3" Grid.Column="2" MinWidth="150 " Background="#FFFF5100" Click="button2_Click" Visibility="Hidden"></Button>
    </Grid>
</Window>
